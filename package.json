{
  "name": "angular-webpack-workflow",
  "version": "1.0.0",
  "description": "A workflow for Angular made with Webpack",
  "scripts": {
    "build": "webpack --config webpack.build.js --bail -p",
    "dev": "webpack-dev-server --history-api-fallback --hot --inline --progress",
    "server": "webpack-dev-server --env dev --history-api-fallback --inline --progress",
    "test": "karma start",
    "test:live": "karma start --auto-watch --no-single-run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Foxandxss/angular-webpack-workflow.git"
  },
  "author": "Jesus Rodriguez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Foxandxss/angular-webpack-workflow/issues"
  },
  "homepage": "https://github.com/Foxandxss/angular-webpack-workflow",
  "dependencies": {
    "angular": "^1.4.3",
    "angular-route": "^1.4.3",
    "angular-ui-router": "^0.2.15"
  },
  "devDependencies": {
    "angular-mocks": "^1.4.3",
    "autoprefixer-core": "^5.2.0",
    "babel-core": "^5.5.8",
    "babel-loader": "^5.1.4",
    "babel-runtime": "^5.5.8",
    "bootstrap": "^3.3.5",
    "css-loader": "^0.14.5",
    "eslint": "^1.0.0",
    "eslint-loader": "^0.14.2",
    "exports-loader": "^0.6.2",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "hamlc-loader": "0.0.11",
    "html-webpack-plugin": "^1.5.2",
    "imports-loader": "^0.6.4",
    "jasmine-core": "^2.3.4",
    "karma": "^0.13.3",
    "karma-chrome-launcher": "^0.2.2",
    "karma-coverage": "^0.4.2",
    "karma-jasmine": "^0.3.6",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.7.0",
    "merge-stream": "^0.1.8",
    "ng-cache-loader": "0.0.9",
    "node-libs-browser": "^0.5.2",
    "null-loader": "^0.1.1",
    "postcss-loader": "^0.4.4",
    "raw-loader": "^0.5.1",
